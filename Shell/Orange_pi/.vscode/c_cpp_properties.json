{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/local/include",
                // linux-headers-(shell uname -r)
                "/usr/src/`uname -r`/include",
                "/usr/src/`uname -r`/arch/x86/include",
                "/usr/src/`uname -r`/include",
                "/usr/src/`uname -r`/arch/x86/include",
                "/usr/src/`uname -r`/arch/x86/include/generated",
                "/usr/lib/gcc/x86_64-linux-gnu/9/include"
            ],
            "defines": [
                "KBUILD_MODNAME=\"hello_module\"",
                "__GNUC__",
                "__KERNEL__"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}